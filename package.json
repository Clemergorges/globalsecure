{
  "name": "globalsecuresend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "postinstall": "prisma generate",
    "start": "next start",
    "lint": "eslint --quiet",
    "typecheck": "tsc --noEmit",
    "verify:code": "npm run lint && npm run typecheck",
    "test": "jest --watch",
    "test:ci": "jest --ci --coverage",
    "test:local": "dotenv -e .env.test -- jest --ci",
    "test:supabase": "dotenv -e .env.supabase.test -- jest --ci",
    "test:unit": "dotenv -e .env.test -- jest --testPathPattern=tests/unit --passWithNoTests",
    "test:integration": "dotenv -e .env.test -- jest --testPathPattern=tests/integration --passWithNoTests",
    "test:e2e": "dotenv -e .env.test -- jest --testPathPattern=tests/e2e",
    "setup:test-db": "powershell -ExecutionPolicy Bypass -File ./scripts/setup-test-db.ps1",
    "test:failure": "dotenv -e .env.test -- jest --ci --runInBand --testPathPattern=tests/failure",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e && npm run test:failure"
  },
